{
  "name": "atlas-api",
  "version": "1.0.0",
  "description": "Atals Restful api",
  "author": "Yassire Elhani <yassire@makeandship.com>",
  "main": "src/index.js",
  "private": true,
  "apidoc": {
    "title": "Atlas api documentation",
    "url": "https://api.atlas-dev.makeandship.com"
  },
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.9",
    "yarn": ">=0.17.9"
  },
  "scripts": {
    "precommit": "yarn lint && yarn test",
    "start": "gulp serve",
    "build": "gulp",
    "lint": "esw *.js src/server src/config src/workers --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "jest src/server/tests/**/*.test.js --coverage --runInBand",
    "test:watch": "jest --watch",
    "compile": "rm -rf dist && babel src --out-dir dist --source-maps",
    "apidoc": "apidoc -i src/server/"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/server",
      "src/config"
    ],
    "collectCoverage": false
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "REST",
    "API",
    "yarn",
    "jest"
  ],
  "dependencies": {
    "async": "^2.1.4",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "crypto": "^0.0.3",
    "debug": "^2.4.5",
    "errors": "0.3.0",
    "express": "4.14.0",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "faker": "^4.1.0",
    "fs-extra": "^2.1.2",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "10.0.6",
    "jsonwebtoken": "7.1.9",
    "lodash": "^4.17.4",
    "method-override": "^2.3.5",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "mongoose": "4.7.4",
    "mongoose-unique-validator": "1.0.3",
    "monq": "https://github.com/yassire/monq.git",
    "morgan": "1.7.0",
    "multer": "^1.3.0",
    "nodemailer": "^3.1.8",
    "nodemailer-mock": "^1.2.1",
    "nodemailer-ses-transport": "^1.5.0",
    "password-hash": "1.2.2",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "url": "^0.11.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "^6.23.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "dirty-chai": "^1.2.2",
    "eslint": "3.12.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "^0.12.0",
    "isparta": "4.0.0",
    "jest": "~18.1.0",
    "request-promise": "^4.2.0",
    "run-sequence": "^1.1.5",
    "sinon": "^2.0.0-pre.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "sourceMaps": true,
    "retainLines": true
  }
}
