{
  "name": "atlas-api",
  "version": "1.0.0",
  "description": "Atals Restful api",
  "author": "Yassire Elhani <yassire@makeandship.com>",
  "main": "src/index.js",
  "private": true,
  "apidoc": {
    "title": "Atlas api documentation",
    "url": "https://api.atlas-dev.makeandship.com"
  },
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.9",
    "yarn": ">=0.17.9"
  },
  "scripts": {
    "precommit": "yarn prettier && yarn test",
    "start": "gulp serve",
    "build": "gulp",
    "prettier": "prettier --find-config-path --write 'src/**/*.js'",
    "prettier-watch": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "test": "jest src/server/tests/**/*.test.js --coverage --verbose --colors",
    "test:watch": "jest --watch",
    "compile": "rm -rf dist && babel src --out-dir dist --source-maps",
    "apidoc": "apidoc -i src/server/"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/server",
      "src/config"
    ],
    "collectCoverage": false,
    "testEnvironment": "node"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "REST",
    "API",
    "yarn",
    "jest"
  ],
  "dependencies": {
    "agenda": "^1.0.3",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "elasticsearch": "^15.0.0",
    "errors": "0.3.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^2.11.0",
    "express-request-id": "^1.4.0",
    "express-validation": "^1.0.2",
    "express-winston": "^2.5.1",
    "faker": "^4.1.0",
    "helmet": "^3.12.1",
    "http-status": "^1.1.0",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.1",
    "method-override": "^2.3.5",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "mongoose": "^5.1.1",
    "mongoose-unique-validator": "^2.0.1",
    "monq": "^0.3.7",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.4",
    "nodemailer-mock": "^1.2.1",
    "nodemailer-ses-transport": "^1.5.0",
    "password-hash": "1.2.2",
    "randomstring": "^1.1.5",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "dirty-chai": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.7",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "mongodb-memory-server": "^1.7.4",
    "nock": "^9.3.2",
    "promised-mongo": "^1.2.0",
    "run-sequence": "^2.2.1",
    "supertest": "^3.1.0"
  }
}
