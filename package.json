{
  "name": "atlas-api",
  "version": "1.0.0",
  "description": "Atals Restful api",
  "author": "Yassire Elhani <yassire@makeandship.com>",
  "main": "src/index.js",
  "private": true,
  "engines": {
    "node": "8.12.0",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "scripts": {
    "precommit": "yarn prettier && yarn test",
    "start": "gulp serve",
    "build": "gulp",
    "prettier": "prettier --find-config-path --write 'src/**/*.js'",
    "prettier-watch": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "test:version": "jest --version",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest -c jest.config.unit.js",
    "test:integration": "jest --verbose --colors --detectOpenHandles -c jest.config.integration.js",
    "test:verbose": "jest -c jest.config.js --coverage --verbose --colors",
    "test:watch": "jest -c jest.config.js  --watch",
    "compile": "rm -rf dist && babel src --out-dir dist --source-maps",
    "postinstall": "cd node_modules/makeandship-api-common && yarn && yarn build"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "REST",
    "API",
    "yarn",
    "jest"
  ],
  "dependencies": {
    "agenda": "^1.0.3",
    "await-spawn": "^2.1.1",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "deepmerge": "^2.1.1",
    "errors": "0.3.0",
    "events": "^3.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "express-request-id": "^1.4.0",
    "express-winston": "^2.5.1",
    "faker": "^4.1.0",
    "flat": "^4.1.0",
    "helmet": "^3.12.1",
    "http-status": "^1.1.0",
    "jsonwebtoken": "^8.2.1",
    "makeandship-api-common": "git+ssh://git@bitbucket.org:makeandship/makeandship-api-common.git#v0.0.14",
    "method-override": "^2.3.5",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "mongoose": "^5.1.1",
    "mongoose-jsonschema-validator": "git+ssh://git@bitbucket.org:makeandship/mongoose-jsonschema-validator.git",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mykrobe-atlas-jsonschema": "git+ssh://git@github.com/Mykrobe-tools/mykrobe-atlas-jsonschema.git",
    "nock": "^9.4.4",
    "nodemailer-ses-transport": "^1.5.0",
    "object-hash": "^1.3.0",
    "password-hash": "1.2.2",
    "randomstring": "^1.1.5",
    "split-file": "^2.1.0",
    "sse-channel": "^3.1.0",
    "swagger-jsdoc": "^1.9.7",
    "swagger-parser": "^5.0.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.7",
    "husky": "^0.14.3",
    "jest": "^24",
    "jest-html-reporter": "^2.4.4",
    "mongodb-memory-server": "^1.7.4",
    "prettier": "^1.16.4",
    "promised-mongo": "^1.2.0",
    "rimraf": "^2.6.2",
    "rmfr": "^2.0.0",
    "run-sequence": "^2.2.1",
    "supertest": "^3.1.0",
    "uuid": "^3.3.2"
  }
}
